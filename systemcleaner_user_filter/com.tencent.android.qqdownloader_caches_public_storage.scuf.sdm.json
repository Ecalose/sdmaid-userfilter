{
	"version": 4,
	"label": "应用宝 缓存 (公共存储)",
	"description": "超过 14 天的缓存",
	"identifier": "com.tencent.android.qqdownloader_caches_public_storage.scuf.sdm",
	"color": "#FF4CAF50",	
	"minimumAge": 1209600000,
	"pathContains": [
