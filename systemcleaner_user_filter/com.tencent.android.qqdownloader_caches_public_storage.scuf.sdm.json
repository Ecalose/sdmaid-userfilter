		"^(?:.+?)(?>com.tencent.android.qqdownloader/files/tassistant/mediaCache/)(?>.+)",		
		"^(?:.+?)(?>com.tencent.android.qqdownloader/files/tassistant/pic/)(?>.+)",
		"^(?:.+?)(?>com.tencent.android.qqdownloader/files/tassistant/video/)(?>.+)"
	],
	"locations": [
		"PUBLIC_DATA"
	]
}