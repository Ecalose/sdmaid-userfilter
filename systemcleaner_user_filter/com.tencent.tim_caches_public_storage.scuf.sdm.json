{
	"version": 4,
	"label": "TIM 缓存 (公共存储)",
	"description": "超过 14 天的缓存",
	"identifier": "com.tencent.tim_caches_public_storage.scuf.sdm",
	"color": "#FF4CAF50",
	"minimumAge": 1209600000,
	"pathContains": [
		"com.tencent.tim/",
		"encent/mini/files/",
		"encent/Tim/"
	],
	"exclusions": [
		".nomedia"
	],
	"regexes": [
		"^(?:.+?)(?>com\\.tencent\\.tim/files/\\.info/)(?:.+)$",
		"^(?:.+?)(?>com\\.tencent\\.tim/qzone/)(?:.+)$",
		"^(?:.+?)(?>T|tencent/mini/files/)(?:.+)$",
		"^(?:.+?)(?>T|tencent/Tim/\\.emotionsm/)(?:.+)$",
		"^(?:.+?)(?>T|tencent/Tim/ArkApp/Cache/)(?:.+)$",
		"^(?:.+?)(?>T|tencent/Tim/photo/)(?:.+)$",
		"^(?:.+?)(?>T|tencent/Tim/thumb/)(?:.+)$",
		"^(?:.+?)(?>T|tencent/Timfile_recv/\\.)(?:.+)$",
		"^(?:.+?)(?>T|tencent/TMAssistantSDK/Download/com\\.tencent\\.tim/)(?:.+)$"
	],
	"locations": [
		"PUBLIC_DATA",
		"SDCARD"
	]
}