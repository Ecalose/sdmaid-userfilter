{
	"version": 4,
	"label": "QQ 缓存 (公共存储)",
	"description": "超过 14 天的缓存",
	"identifier": "com.tencent.mobileqq_caches_public_storage.scuf.sdm",
	"color": "#FF4CAF50",
	"minimumAge": 1209600000,
	"pathContains": [
		"com.tencent.mobileqq/",
		"encent/mini/files/",
		"encent/MobileQQ/"
