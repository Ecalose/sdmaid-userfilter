	"exclusions": [
		".nomedia"
	],
	"regexes": [
		"^(?:.+?)(?>com\\.tencent\\.mobileqq/files/\\.info/)(?:.+)$",
		"^(?:.+?)(?>com\\.tencent\\.mobileqq/files/uploader/)(?:.+)$",
		"^(?:.+?)(?>com\\.tencent\\.mobileqq/qzone/)(?:.+)$",
		"^(?:.+?)(?>T|tencent/mini/files/)(?:.+)$",
		"^(?:.+?)(?>T|tencent/MobileQQ/\\.emotionsm/)(?:.+)$",
		"^(?:.+?)(?>T|tencent/MobileQQ/\\.gift/)(?:.+)$",
		"^(?:.+?)(?>T|tencent/MobileQQ/ArkApp/Cache/)(?:.+)$",
		"^(?:.+?)(?>T|tencent/MobileQQ/diskcache/)(?:.+)$",
		"^(?:.+?)(?>T|tencent/MobileQQ/pddata/app/offline/)(?:.+)$",
		"^(?:.+?)(?>T|tencent/MobileQQ/photo/)(?:.+)$",
		"^(?:.+?)(?>T|tencent/MobileQQ/Scribble/ScribbleCache/)(?:.+)$",
		"^(?:.+?)(?>T|tencent/MobileQQ/thumb/)(?:.+)$",
		"^(?:.+?)(?>T|tencent/MobileQQ/webso/offline/)(?:.+)$",
		"^(?:.+?)(?>T|tencent/QQfile_recv/\\.)(?:.+)$",
		"^(?:.+?)(?>T|tencent/TMAssistantSDK/Download/com\\.tencent\\.mobileqq/)(?:.+)$"		
	],
	"locations": [
		"PUBLIC_DATA",
		"SDCARD"
	]
}